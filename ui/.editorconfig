; This file is for unifying the coding style for different editors and IDEs.
; More information at http://editorconfig.org

root = true

[*]
indent_style = tab
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[Makefile]
trim_trailing_whitespace = false

[*.md]
indent_style = space
indent_size = 2
trim_trailing_whitespace = false

[.ebextensions/*.config]
indent_style = space
indent_size = 2

[**/*.{yaml,yml}]
indent_style = space
indent_size = 2

[*.{babelrc,jq,json,toml}]
indent_style = space
indent_size = 2
