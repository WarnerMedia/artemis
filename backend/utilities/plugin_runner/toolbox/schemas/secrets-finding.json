{
  "$id": "https://wbd.com/artemis/plugin/secrets-finding.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",

  "title": "SecretsFinding",
  "type": "object",
  "required": [
    "filename",
    "line",
    "commit",
    "type",
    "author"
  ],
  "properties": {
    "filename": {
      "type": "string"
    },
    "line": {
      "type": "integer"
    },
    "commit": {
      "type": "string"
    },
    "type": {
      "enum": [
        "aws",
        "ssh",
        "mongo",
        "postgres",
        "redis",
        "urlauth",
        "google",
        "slack",
        "other"
      ]
    },
    "author": {
      "type": "string"
    }
  }
}
